services:
  minimalapi:
    build:
      context: /Users/josemariahernandez/Projects/MinimalAPI
      dockerfile: MinimalAPI/Dockerfile
      labels:
        com.microsoft.created-by: visual-studio
        com.microsoft.visual-studio.project-name: MinimalAPI
      target: base
    container_name: MinimalAPI
    entrypoint: tail -f /dev/null
    environment:
      ASPNETCORE_ENVIRONMENT: Development
      ASPNETCORE_LOGGING__CONSOLE__DISABLECOLORS: "true"
      DOTNET_USE_POLLING_FILE_WATCHER: '1'
      NUGET_FALLBACK_PACKAGES: /root/.nuget/fallbackpackages
    image: minimalapi:dev
    labels:
      com.microsoft.visualstudio.debuggee.arguments: ' --additionalProbingPath /root/.nuget/packages
        --additionalProbingPath /root/.nuget/fallbackpackages  "/app/bin/Debug/net6.0/MinimalAPI.dll"'
      com.microsoft.visualstudio.debuggee.killprogram: /bin/sh -c "if PID=$$(pidof
        dotnet); then kill $$PID; fi"
      com.microsoft.visualstudio.debuggee.program: dotnet
      com.microsoft.visualstudio.debuggee.workingdirectory: /app
    ports:
    - target: 80
    tty: true
    volumes:
    - /Users/josemariahernandez/Projects/MinimalAPI/MinimalAPI:/app:rw
    - /Users/josemariahernandez/vsdbg/vs2017u5:/remote_debugger:rw
    - /usr/local/share/dotnet/sdk/NuGetFallbackFolder:/root/.nuget/fallbackpackages:ro
    - /Users/josemariahernandez/.nuget/packages:/root/.nuget/packages:ro
    - /Users/josemariahernandez/Projects/MinimalAPI:/src:rw
version: '3.4'